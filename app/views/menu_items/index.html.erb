<div style="width:88%;margin:0 auto;padding:2%;padding-left:5%;">
<p id="notice"><%= notice %></p>
<h1>Listing Bottom Text at Footer</h1>
<% @logo_texts.each do |logo_text| %>
<div class="well" style="float:left;margin-right:4px;">
	<h3><%= logo_text.logo_bottom_text %></h3>
	<p><%= logo_text.url %></p>
	<div class="well"><p><%= link_to 'Show', logo_text %> | <%= link_to 'Edit', edit_logo_text_path(logo_text) %> | <%= link_to 'Destroy', logo_text, method: :delete, data: { confirm: 'Are you sure?' } %></p></div>

</div>
<%end%>
<div class="panel" style="padding:5px;width:auto;background-color:#ef403d; color:#fff;"><%= link_to 'New Logo text', new_logo_text_path,class: "olol" %></div>

<br>
<br><br>
<br>
<hr>


<h1>Listing Menu Items</h1>



<br>
<% @menu_items.each do |menu_item| %>

<div class="panel panel-info">
  <div class="panel-heading"><h3><%= menu_item.title.upcase %></h3></div>
  <div class="panel-body"><p><%= menu_item.url %></p></div>
  <div class="well"><p><%= link_to 'Show', menu_item %> | <%= link_to 'Edit', edit_menu_item_path(menu_item) %> | <%= link_to 'Destroy', menu_item, method: :delete, data: { confirm: 'Are you sure?' } %></p></div>
</div>
<% end %>
<div class="panel" style="padding:5px;width:auto;background-color:#009688;"><%= link_to 'New Menu item', new_menu_item_path, class: "olol" %></div>
</div>